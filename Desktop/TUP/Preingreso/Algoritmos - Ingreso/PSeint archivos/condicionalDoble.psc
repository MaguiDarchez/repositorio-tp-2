Algoritmo condicionalDoble
	
	Definir numeroA, numeroB Como Entero;
	
	Escribir "Ingrese el valor A:";
	Leer numeroA;
	
	Escribir "Ingrese el valor de B:";
	Leer numeroB;
	
	si numeroA > numeroB Entonces
		Escribir "El numero mayor es ", numeroA;
	sino 
		Escribir "El numero mayor es ", numeroB;
	FinSi
	
FinAlgoritmo
